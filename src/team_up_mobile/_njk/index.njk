{# 选手 #}
{% macro
rank(item) %}
  <div class="rank-item">
    <div class="rank-avator-box">
      <div class="rank-img" data-id="rank-{{ item.id }}">
        <img src="{{ item.img }}"/>
      </div>
      <img src="./img/rankinglist.png" alt="" class="rank-rankinglist"/>
      <span class="rank-standing">{{ item.id }}</span>
      <span class="rank-name">{{ item.name }}</span>
    </div>
    <span class="rank-btn" data-id="rank-{{ item.id }}">{{text('vote')}}</span>
  </div>

{% endmacro %}

<!-- 内容区 -->
{% block main %}
  <div class="teamup">
    <!-- 头部 -->
    <div class="teamup-header">
      <div class="swiper-container swiper-header">
        <!-- 轮播图片 当前为自动轮播 当只有一张时停止自动轮播 -->
        <div class="swiper-wrapper">
          <!-- 轮播图片 当前为自动轮播 当只有一张时停止自动轮播 -->
          <div class="swiper-slide"><img src="./img/teamup_header_bg.jpg" alt=""  onclick="return false">
            <!-- 头部 logo -->
            <div class="logo-box">
              <img src="./img/logo.png" alt="" class='logo'  onclick="return false">
            </div>
            <!-- 头部 title -->
            <div class="title-box">
              <img src="./img/title.png" alt="" class='title'  onclick="return false">
            </div>
          </div>
          <div class="swiper-slide"><img src="./img/teamup_header_bg2.jpg" alt=""  onclick="return false">
            <!-- 头部 logo -->
            <div class="logo-box">
              <img src="./img/logo2.png" alt="" class='logo'  onclick="return false">
            </div>
            <!-- 头部 title -->
            <div class="title-box">
              <img src="./img/title2.png" alt="" class='title'  onclick="return false">
            </div>
          </div>
        </div>

        <div class="simplelogo-box">
          <a href="javascript:void(0)"><img src="./img/simple_logo.png" alt="" class='simplelogo'></a>
        </div>
        <!-- 头部 玩家id || 玩家选票数 -->
        <div class="vote-box">
          <p>{{text('id')}}
            <span id="player-id">123456789</span></p>
          <p>{{text('residue')}}
            <span id="vote-num">8</span></p>
        </div>
        <!-- 活动介绍 -->
        <div class='text-box'>
          <p class='text'>{{text('pick')}}</p>
          <p class='text'>
            {{text('giver')}}</p>
        </div>
      </div>
    </div>
    <!-- 内容区 -->
    <div class="teamup-content">
      <div class="teamup-rules">{{text('rules')}}</div>
      <div class="teamup-tabbar-wrapper">
        <div class="teamup-tabbar-affix">
          <div id="top">
            <!-- 导航栏 -->
            <div class="tabbar">
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_all'});" class="tab-item active">{{text('all')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_mena'});" class="tab-item vfx">{{text('mena')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_na'});" class="tab-item">{{text('n_a')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_eu'});" class="tab-item">{{text('europe')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_jp'});" class="tab-item">{{text('jp')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_kr'});" class="tab-item">{{text('korea')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_in'});" class="tab-item">{{text('india')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_th'});" class="tab-item">{{text('thailand')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_vi'});" class="tab-item">{{text('vietnam')}}</span>
              <span onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_nation_indonesia'});" class="tab-item">{{text('indonesia')}}</span>
            </div>
          </div>
          <div class="arrows"></div>
        </div>
      </div>
      <div class="swiper-container" id="page">
        <div class="swiper-wrapper">
          <!-- 全部 -->
          <div class="swiper-slide slidepage">
            <div class="rank-box">
              <div  class="rank-content-box">
                {% for item in allList %}
                  {{ rank(item) }}

                {% endfor %}
              </div>

            </div>
          </div>
          <!-- 中东 -->
          <div class="swiper-slide slidepage">
            <div class="rank-box">

              <div  class="rank-content-box">
                {% for item in eameList %}
                  {{ rank(item) }}

                {% endfor %}
              </div>

            </div>
          </div>
          <!-- 北美 -->
          <div class="swiper-slide slidepage">
            <div class="rank-box">
              <div  class="rank-content-box">
                {% for item in americaList %}
                  {{ rank(item) }}

                {% endfor %}
              </div>

            </div>
          </div>
          <!-- 欧洲 -->
          <div class="swiper-slide slidepage">
            <div class="rank-box">
              <div  class="rank-content-box">
                {% for item in europeList %}
                  {{ rank(item) }}

                {% endfor %}
              </div>

            </div>
          </div>
          <!-- 日本 -->
          <div class="swiper-slide slidepage">
