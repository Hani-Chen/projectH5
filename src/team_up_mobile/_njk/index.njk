    </div>
  </div>
  {# 页面所需所有弹窗 #}

  <!-- 活动规则弹窗 -->
  <div class="pop" data-rule>
    <div class="pop-rule-content">
      <h1 class="pop-title">{{text('rules')}}</h1>
      <ul class="rule-list">
        <li>{{text('presentation')}}</li>
        <li>{{text('presentation1')}}</li>
        <li>{{text('presentation2')}}</li>
        <li>{{text('presentation3')}}</li>
        <li>{{text('presentation4')}}</li>
        <li>{{text('presentation5')}}</li>
        <li>{{text('presentation6')}}</li>
        <li>{{text('presentation7')}}</li>
        <li>{{text('presentation8')}}</li>
        <li>{{text('presentation9')}}</li>
        <li>{{text('presentation10')}}</li>
      </ul>
      <img src="./img/shut.png" alt=""  class="pop-shut" data-shut/>
    </div>
  </div>

  <!-- 活动结束 弹窗 -->
  <div class="pop" data-over>
    <div class="pop-none-content">
      <p class="pop-none-text">当前活动已经结束</p>
      <img src="./img/shut.png" alt="" class="pop-shut"  onclick="return false" data-shut/>
    </div>
  </div>

  <!-- 操作错误 弹窗 -->
  <div class="pop" data-err>
    <div class="pop-none-content">
      <p class="pop-none-text">操作错误</p>
      <img src="./img/shut.png" alt="" class="pop-shut"  onclick="return false" data-shut/>
    </div>
  </div>

  <!-- 没有选票弹窗 -->
  <div class="pop" data-none>
    <div class="pop-none-content">
      <p class="pop-none-text">{{text('gone')}}</p>
      <img src="./img/shut.png" alt="" class="pop-shut"  onclick="return false" data-shut/>
    </div>
  </div>

  <!-- 分享弹窗 -->
  <div class="pop" data-share>
    <div class="pop-share-content">
      <p class="pop-share-text">{{text('share_via')}}</p>
      <div class="pop-share-iconbox">
        <!-- facebook分享 参数u为要分享的页面链接 
      需要在对应的页面网页meta属性中配置以下facebook分享属性项：
      <meta property="og:title" content="The Rock"/>分享标题
      <meta property="og:image" content="http://ia.media-imdb.com/rock.jpg"/>分享图片
      <meta property="og:description" content="blar blar blar blar blar"/>分享描述
     -->
        <a
        href="https://www.facebook.com/sharer/sharer.php?u=https://google.com"
        class="header-right-facebook"
        target="_blank" onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_share_facebook'});"
      >
          <img src="./img/share_1.png" alt=""/>
        </a>
        <!-- twitter 需配置链接中的url属性和text属性 
      可参考 https://developer.twitter.com/en/docs/twitter-for-websites/tweet-button/guides/web-intent.html
     -->
        <a
        href="https://twitter.com/intent/tweet?url=https://google.com&text=分享標題"
        class="header-right-twitter"
        target="_blank" onclick="gtag('event', 'x_jump', {'event_category': '20190617teamup','event_label': 'm_share_twitter'});"
      >
          <img src="./img/share_2.png" alt=""/>
        </a>
      </div>
      <img src="./img/shut.png" alt="" class="pop-shut" data-shut  onclick="return false"/>
    </div>
  </div>

  <!-- 点赞弹窗 -->
  <div class="pop" data-like>
    <div class="pop-like-content">
      <div class="pop-like-header">
        <img src="./img/rink2.jpg" class="pop-like-img"  onclick="return false"/>
        <span class="pop-like-name">Alan Walker</span>
      </div>
      <div class="pop-like-middle">
        <p class="pop-like-text">
          {{text('raid')}}<br/>
          {{text('mail')}}
        </p>

      </p>
      <img src="./img/tu.jpg" alt="" class="pop-like-treasure"  onclick="return false"/>
    </div>
    <div class="pop-like-bottom">
      <span class="pop-like-button" data-continue>{{text('continue')}}</span>
      <span  onclick="gtag('event','x_click',{'event_category':'20190617teamup','event_label':'m_share_btn'});" class="pop-like-button" data-shareplay>{{text('share')}}</span>
    </div>
    <img src="./img/shut.png" alt="" class="pop-shut" data-shut  onclick="return false"/>
