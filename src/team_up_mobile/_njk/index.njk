{% extends "./base.njk" %}
{% import "./lang.njk" as lang %}
{% set langV = lang[langCode] %}

{% macro text(key) %}
  {% if langOutput %}
    {% raw %}
      {# {% endraw %}{{key | safe}}{% raw %} #}
    {% endraw %}
  {% else %}{{langV[key] | safe}}
  {% endif %}
{% endmacro %}
