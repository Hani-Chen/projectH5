{# 选手 #}
{% macro
rank(item) %}
  <div class="rank-item">
    <div class="rank-avator-box">
      <div class="rank-img" data-id="rank-{{ item.id }}">
        <img src="{{ item.img }}"/>
      </div>
      <img src="./img/rankinglist.png" alt="" class="rank-rankinglist"/>
      <span class="rank-standing">{{ item.id }}</span>
      <span class="rank-name">{{ item.name }}</span>
    </div>
    <a href="javascript:;">
      <span class="rank-btn" data-id="rank-{{ item.id }}">{{text('vote')}}</span></a>
  </div>

{% endmacro %}

